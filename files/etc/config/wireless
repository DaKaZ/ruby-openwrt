config wifi-device  radio0
	option type     mac80211
	option channel  11
	option macaddr	00:1a:2b:09:5e:15
	option hwmode	11g

	# REMOVE THIS LINE TO ENABLE WIFI:
	# option disabled 1

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     R.Chat
	option encryption none

